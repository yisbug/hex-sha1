{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "no-mixed-operators": 0,
    "no-continue": 0,
    "no-await-in-loop": 0,
    "max-len": 0,
    "eol-last": 0,
    "no-param-reassign": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "import"
  ]
}